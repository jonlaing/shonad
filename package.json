{
  "name": "shonad",
  "version": "1.0.0",
  "description": "Functional helper library to make Javascript and Typescript programs more predictable",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./base": "./dist/base/index.js",
    "./control": "./dist/control/index.js",
    "./data": "./dist/data/index.js",
    "./extensions": "./dist/data/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "base": [
        "dist/base/index.d.ts"
      ],
      "control": [
        "dist/control/index.d.ts"
      ],
      "data": [
        "dist/data/index.d.ts"
      ],
      "extensions": [
        "dist/extensions/index.d.ts"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@microsoft/tsdoc": "^0.14.1",
    "@microsoft/tsdoc-config": "^0.16.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-toolbelt": "^9.6.0",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.6.4",
    "uglify-js": "^3.15.5"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc -p tsconfig.build.json",
    "build:docs:md": "typedoc --options typedoc.md.json",
    "build:docs:html": "typedoc --options typedoc.html.json",
    "uglify": "find ./dist/  -iname '*.js' -exec uglifyjs --compress --comments /sourceMappingURL/ -o {} -- {} \\;"
  }
}
